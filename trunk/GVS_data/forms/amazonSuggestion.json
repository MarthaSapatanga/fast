    "actions": [{"name":"init", "preconditions":[]},
	          {"name":"showTable", "preconditions":[{"id":"list", "pattern":"?PList http://www.w3.org/1999/02/22-rdf-syntax-ns#type http://aws.amazon.com/AWSECommerceService#ProductList", "positive":"true"}]},
	           {"name":"showMessage", "preconditions":[{"id":"message", "pattern":"?Message http://www.w3.org/1999/02/22-rdf-syntax-ns#type http://aws.amazon.com/AWSECommerceService#Message", "positive":"true"}]}],
	"postconditions": [{"id":"item", "pattern":"?Item http://www.w3.org/1999/02/22-rdf-syntax-ns#type http://aws.amazon.com/AWSECommerceService#Item", "positive":"true"}],
	"triggers":["item"]
