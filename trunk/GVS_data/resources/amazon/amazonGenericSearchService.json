    "postconditions":[
        [
            {
                "id":"list",
                "label":{
                    "en-gb":"A Generic Item List"
                },
                "pattern":"?ItemGroup http://www.w3.org/1999/02/22-rdf-syntax-ns#type http://aws.amazon.com/AWSECommerceService#ItemGroup",
                "positive":"true"
            }
        ]
    ],
    "triggers":[
        "itemList"
    ]
