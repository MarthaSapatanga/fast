{
    "name":"Compound Amazon List Screen",
    "creationDate":"2009-02-11T14:59:52+0100",
    "creator":"http://www.ismaelrivera.es/",
    "description":{
        "en-gb":"This screen shows the results of a given search in Amazon. It allows users to choose a product to see its details."
    },
    "domainContext":{
        "tags":[
            "compound",
            "amazon",
            "list"
        ],
        "user":"admin"
    },
    "homepage":"http://www.deri.ie/amazonList",
    "icon":"http://demo.fast.morfeo-project.org/gvsdata/images/catalogue/amazonList.png",
    "label":{
        "en-gb":"Product List Compound"
    },
    "rights":"http://creativecommons.org/",
    "screenshot":"http://demo.fast.morfeo-project.org/gvsdata/images/screenshots/amazonProductList.png",
    "version":"1.0",
    "definition":{
        "buildingblocks":[
            {
                "id":"amazonListForm1",
                "uri":"http://purl.oclc.org/fast/ontology/gadget#FormElement1263313163770"
            },
            {
                "id":"AmazonSearchService1",
                "uri":"http://purl.oclc.org/fast/ontology/gadget#BackendService1263313249397"
            }
        ],
        "pipes":[
            {
                "from":{
                    "buildingblock":"",
                    "condition":"filter"
                },
                "to":{
                    "buildingblock":"AmazonSearchService1",
                    "condition":"filter",
                    "action":"search"
                }
            },
            {
                "from":{
                    "buildingblock":"AmazonSearchService1",
                    "condition":"list"
                },
                "to":{
                    "buildingblock":"amazonListForm1",
                    "condition":"list",
                    "action":"showTable"
                }
            },
            {
                "from":{
                    "buildingblock":"amazonListForm1",
                    "condition":"filter"
                },
                "to":{
                    "buildingblock":"AmazonSearchService1",
                    "condition":"filter",
                    "action":"search"
                }
            },
            {
                "from":{
                    "buildingblock":"amazonListForm1",
                    "condition":"item"
                },
                "to":{
                    "buildingblock":"",
                    "action":"",
                    "condition":"item"
                }
            }
        ],
        "triggers":[
            {
                "from":{
                    "buildingblock":"",
                    "name":"_onload"
                },
                "to":{
                    "buildingblock":"amazonListForm1",
                    "action":"init"
                }
            },
            {
                "from":{
                    "buildingblock":"",
                    "name":"_onload"
                },
                "to":{
                    "buildingblock":"AmazonSearchService1",
                    "action":"search"
                }
            }
        ]
    },
    "preconditions":[
        [
            {
                "id":"filter",
                "label":{
                    "en-gb":"A search criteria"
                },
                "pattern":"?F http://www.w3.org/1999/02/22-rdf-syntax-ns#type http://aws.amazon.com/AWSECommerceService#SearchCriteria",
                "positive":"true"
            }
        ]
    ],
    "postconditions":[
        [
            {
                "id":"item",
                "label":{
                    "en-gb":"An item"
                },
                "pattern":"?I http://www.w3.org/1999/02/22-rdf-syntax-ns#type http://aws.amazon.com/AWSECommerceService#Item",
                "positive":"true"
            }
        ]
    ]
}