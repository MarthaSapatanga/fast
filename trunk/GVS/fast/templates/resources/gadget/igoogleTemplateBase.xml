<?xml version="1.0" encoding="UTF-8" ?> 
<Module>
<ModulePrefs title="{{ gadgetTitle }}"
        title_url="{{ gadgetTemplateURI }}"
        author="{{ gadgetAuthor }}"
        author_email="{{ gadgetMail }}"
        description="{{ gadgetDescription }}"
        scrolling="true" height="400">
    <Require feature="dynamic-height"/>
</ModulePrefs>
<Content type="html">
<![CDATA[
{% block content %}{% endblock %}
]]>
</Content>
</Module>
